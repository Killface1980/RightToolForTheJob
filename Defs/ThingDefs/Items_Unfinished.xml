<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <selectable>true</selectable>
    <tradeability>Never</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <!-- For CompForbiddable -->
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
    </statBases>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>


  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedWheelChair</defName>
    <label>Unfinished wheel chair</label>
    <graphicData>
      <texPath>Things/Vehicles/VehicleSmackWay/SmackWay</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  
  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedCart</defName>
